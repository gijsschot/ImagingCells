@article{Persson2001,
abstract = {Photoionization of an atom by X-rays usually removes an inner shell electron from the atom, leaving behind a perturbed "hollow ion" whose relaxation may take different routes. In light elements, emission of an Auger electron is common. However, the energy and the total number of electrons released from the atom may be modulated by shake-up and shake-off effects. When the inner shell electron leaves, the outer shell electrons may find themselves in a state that is not an eigen-state of the atom in its surroundings. The resulting collective excitation is called shake-up. If this process also involves the release of low energy electrons from the outer shell, then the process is called shake-off. It is not clear how significant shake-up and shake-off contributions are to the overall ionization of biological materials like proteins. In particular, the interaction between the outgoing electron and the remaining system depends on the chemical environment of the atom, which can be studied by quantum chemical methods. Here we present calculations on model compounds to represent the most common chemical environments in proteins. The results show that the shake-up and shake-off processes affect approximately 20{\%} of all emissions from nitrogen, 30{\%} from carbon, 40{\%} from oxygen, and 23{\%} from sulfur. Triple and higher ionizations are rare for carbon, nitrogen, and oxygen, but are frequent for sulfur. The findings are relevant to the design of biological experiments at emerging X-ray free-electron lasers.},
author = {Persson, Petter and Lunell, Sten and Sz{\"{o}}ke, Abraham and Ziaja, Beata and Hajdu, Janos},
doi = {10.1110/ps.ps.26201},
issn = {09618368},
journal = {Protein Science},
month = {mar},
number = {12},
pages = {2480--2484},
pmid = {11714915},
title = {{Shake-up and shake-off excitations with associated electron losses in X-ray studies of proteins}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11714915 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2374029 http://doi.wiley.com/10.1110/ps.ps.26201},
volume = {10},
year = {2001}
}
@article{Giannakis2010,
abstract = {We perceive the world through images formed by scatte-ring. The ability to interpret scattering data mathematically has opened to our scrutiny the constituents of matter, the building blocks of life, and the remotest corners of the universe. Here, we present an approach to image formation based on the symmetry properties of operations in three-dimensional space. Augmented with graph-theoretic means, this approach can recover the three-dimensional structure of objects from random snapshots of unknown orientation at four orders of magnitude higher complexity than previously demonstrated. This is critical for the burgeoning field of structure recovery by X-ray Free Electron Lasers, as well as the more established electron microscopic techniques, including cryo-electron microscopy of biological systems. In a subsequent paper, we demonstrate the recovery of structure and dynamics from experimental, ultralow-signal random sightings of systems with X-rays, electrons, and photons, with no orientational or timing information. OCIS codes: (290.5825) Scattering theory; (290.5840) Scattering, molecules; (290.3200) In-verse scattering; (140.2600) Free-electron lasers (FELs); (180.6900) Three-dimensional mi-croscopy.},
author = {Giannakis, Dimitrios and Schwander, Peter and Ourmazd, Abbas},
file = {:Users/Goldmund/Library/Application Support/Mendeley Desktop/Downloaded/Giannakis et al. - 2012 - The symmetries of image formation by scattering. I. Theoretical framework.pdf:pdf},
journal = {Optics express},
number = {12},
pages = {5591--5596},
title = {{The symmetries of image formation by scattering. I. Theoretical framework}},
