pmid = {10963603},
title = {{Potential for biomolecular imaging with femtosecond X-ray pulses}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10963603 http://www.nature.com/doifinder/10.1038/35021099},
volume = {406},
year = {2000}
